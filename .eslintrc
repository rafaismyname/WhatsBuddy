{
  "extends": ["airbnb-base"],
  "env": { "browser": true },
  "rules": {
    "no-console": ["error", { allow: ["warn", "error"] }],
    "func-names": ["error", "as-needed"],
    "max-len": ["error", { "code": 160 }],
    "max-classes-per-file": ["error", 3]
  },
  "globals": {
   "browser": true,
   "chrome": true,
   "window": true,
   "document": true
  }
}
